<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="uca.edu.simpsonsquotes">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:name="uca.edu.simpsonsquotes.ui.MainApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_donut"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_donut"
        android:supportsRtl="true"
        android:theme="@style/Theme.SimpsonsQuotes">
        <activity
            android:name=".ui.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.SimpsonsQuotes.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
</manifest>